<!DOCTYPE html>
<html>
<head>
    <link href="~/Content/normalize.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/datepicker.css" rel="stylesheet" type="text/css" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Libero RENT A CAR</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


</head>
<body>
    <div id="header">
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    @Html.ActionLink("Libero Rent A Car", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("Chat", "Chat", "Home")</li>
                        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                        <li>@Html.ActionLink("About", "About", "Home")</li>
                    </ul>
                    @Html.Partial("_LoginPartial")
                </div>
            </div>
        </div>
    @*</div>*@

    <div id="content">
        @RenderBody()
        <footer>
            <p>&copy; @DateTime.Now.Year - Libero Rent A Car</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jqueryui")
    @Styles.Render("~/Content/cssjqryUi")
    <script src="@Url.Content("~/Scripts/Date.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Ready.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Teste.js")" type="text/javascript"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
